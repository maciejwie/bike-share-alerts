{
    "version": 2,
    "builds": [
        {
            "src": "backend/api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "backend/collector/main.go",
            "use": "@vercel/go"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/api/index.py"
        },
        {
            "src": "/collector",
            "dest": "/backend/collector/main.go"
        }
    ]
}